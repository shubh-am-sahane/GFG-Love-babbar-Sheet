{
        // Your code goes here
        int arr2[n];
        int positive=0;
        for(int i=0;i<n;i++){
            if(arr[i]>0){
                positive++;
            }
        }
        int k=0;
        for(int i=0;i<n;i++){
            if(arr[i]>0){
                arr2[k]=arr[i];
                k++;
            }
            else{
                arr2[positive]=arr[i];
                positive++;
            }
        }
        for(int i=0;i<n;i++){
            arr[i]=arr2[i];
        }    
    }
